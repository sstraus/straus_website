          "text": "     1→/**\n     2→ * OutputRenderer - Renders output to the terminal\n     3→ */\n     4→import { createElement, scrollToBottom, clearChildren } from '../utils/dom.js';\n     5→import { TypeWriter } from '../effects/TypeWriter.js';\n     6→import { delay } from '../utils/delay.js';\n     7→import { config } from '../config.js';\n     8→\n     9→export class OutputRenderer {\n    10→  /**\n    11→   * @param {HTMLElement} outputContainer\n    12→   */\n    13→  constructor(outputContainer) {\n    14→    this.container = outputContainer;\n    15→    this.typewriter = new TypeWriter();\n    16→  }\n    17→\n    18→  /**\n    19→   * Print a line of text (instant)\n    20→   * @param {string} text\n    21→   * @param {string} type - 'normal', 'system', 'error', 'success', 'info', 'command'\n    22→   */\n    23→  print(text, type = 'normal') {\n    24→    const line = createElement('div', {\n    25→      className: `terminal-line terminal-line--${type}`,\n    26→    }, text);\n    27→\n    28→    this.container.appendChild(line);\n    29→    this.scrollToEnd();\n    30→  }\n    31→\n    32→  /**\n    33→   * Print multiple lines\n    34→   * @param {string[]} lines\n    35→   * @param {string} type\n    36→   */\n    37→  printLines(lines, type = 'normal') {\n    38→    for (const line of lines) {\n    39→      this.print(line, type);\n    40→    }\n    41→  }\n    42→\n    43→  /**\n    44→   * Type text with animation\n    45→   * @param {string} text\n    46→   * @param {string} type\n    47→   * @param {Object} options\n    48→   * @returns {Promise<void>}\n    49→   */\n    50→  async typewrite(text, type = 'normal', options = {}) {\n    51→    const line = createElement('div', {\n    52→      className: `terminal-line terminal-line--${type}`,\n    53→    });\n    54→\n    55→    this.container.appendChild(line);\n    56→\n    57→    await this.typewriter.type(text, line, options);\n    58→    this.scrollToEnd();\n    59→  }\n    60→\n    61→  /**\n    62→   * Type multiple lines with animation\n    63→   * @param {string[]} lines\n    64→   * @param {string} type\n    65→   * @returns {Promise<void>}\n    66→   */\n    67→  async typewriteLines(lines, type = 'normal') {\n    68→    for (const line of lines) {\n    69→      await this.typewrite(line, type);\n    70→      await delay(config.delays.lineBreak);\n    71→    }\n    72→  }\n    73→\n    74→  /**\n    75→   * Print the command echo (shows what user typed)\n    76→   * @param {string} command\n    77→   */\n    78→  printCommand(command) {\n    79→    const line = createElement('div', { className: 'command-echo' },\n    80→      createElement('span', { className: 'prompt-symbol' }),\n    81→      createElement('span', { className: 'command-text' }, command)\n    82→    );\n    83→\n    84→    this.container.appendChild(line);\n    85→    this.scrollToEnd();\n    86→  }\n    87→\n    88→  /**\n    89→   * Render HTML content (for markdown)\n    90→   * @param {string} html\n    91→   * @param {string} className\n    92→   */\n    93→  renderHtml(html, className = 'md-content') {\n    94→    const wrapper = createElement('div', { className });\n    95→    wrapper.innerHTML = html;\n    96→    this.container.appendChild(wrapper);\n    97→    this.scrollToEnd();\n    98→  }\n    99→\n   100→  /**\n   101→   * Print ASCII art\n   102→   * @param {string[]} lines\n   103→   */\n   104→  printAscii(lines) {\n   105→    const container = createElement('div', { className: 'terminal-line terminal-line--ascii' });\n   106→    container.innerHTML = lines.join('\\n');\n   107→    this.container.appendChild(container);\n   108→    this.scrollToEnd();\n   109→  }\n   110→\n   111→  /**\n   112→   * Print a divider line\n   113→   */\n   114→  printDivider() {\n   115→    this.print('─'.repeat(50), 'divider');\n   116→  }\n   117→\n   118→  /**\n   119→   * Print suggestions\n   120→   * @param {Array<{label: string, command: string}>} suggestions\n   121→   * @param {Function} onClick\n   122→   */\n   123→  printSuggestions(suggestions, onClick) {\n   124→    const container = createElement('div', { className: 'suggestions' });\n   125→\n   126→    for (const { label, command } of suggestions) {\n   127→      const btn = createElement('button', {\n   128→        className: 'suggestion',\n   129→        onClick: () => onClick(command),\n   130→      }, label);\n   131→\n   132→      container.appendChild(btn);\n   133→    }\n   134→\n   135→    this.container.appendChild(container);\n   136→    this.scrollToEnd();\n   137→  }\n   138→\n   139→  /**\n   140→   * Print an error message\n   141→   * @param {string} message\n   142→   */\n   143→  error(message) {\n   144→    this.print(`Error: ${message}`, 'error');\n   145→  }\n   146→\n   147→  /**\n   148→   * Print a success message\n   149→   * @param {string} message\n   150→   */\n   151→  success(message) {\n   152→    this.print(message, 'success');\n   153→  }\n   154→\n   155→  /**\n   156→   * Clear all output\n   157→   */\n   158→  clear() {\n   159→    clearChildren(this.container);\n   160→  }\n   161→\n   162→  /**\n   163→   * Scroll to the end of output\n   164→   */\n   165→  scrollToEnd() {\n   166→    scrollToBottom(this.container);\n   167→  }\n   168→\n   169→  /**\n   170→   * Print empty line\n   171→   */\n   172→  newline() {\n   173→    this.print('');\n   174→  }\n   175→}\n   176→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n",
